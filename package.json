    "lint": "eslint src",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@immowelt/eslint-config-immowelt-base": "1.1.0",
    "@immowelt/jest-preset-node": "1.1.1",
    "browserslist": "4.16.5",
    "eslint": "5.7.0",
    "jest": "23.6.0",
    "semantic-release": "17.2.3"
  },
  "engines": {
    "node": ">=8.11.4",
    "yarn": ">=1.6.0"
  },
  "keywords": [
    "browserslist",
    "immowelt",
    "config",
    "immonet"
  ],
  "files": [
    "src/index.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "release": {
